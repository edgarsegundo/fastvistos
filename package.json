{
  "name": "fastvistos",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "msitesapp-dev": "node --trace-warnings ./multi-sites/core/msitesapp/server.js",
    "msitesapp-start": "node ./multi-sites/core/msitesapp/server.js",
    "msitesapp-debug": "node --inspect=0.0.0.0:9229 --trace-warnings ./multi-sites/core/msitesapp/server.js",
    "msitesapp-debug-brk": "node --inspect-brk=0.0.0.0:9229 --trace-warnings ./multi-sites/core/msitesapp/server.js",
    "postbuild:msitesapp": "cp ./multi-sites/core/lib/*.js ./multi-sites/core/dist/lib/",
    "build:msitesapp": "tsc -p ./multi-sites/core/msitesapp/tsconfig.json && npm run postbuild:msitesapp",
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:js": "prettier --write \"**/*.{js,mjs,ts,jsx,tsx}\"",
    "format:astro": "prettier --write '**/*.astro' --plugin=prettier-plugin-astro",
    "format:md": "prettier --write \"**/*.md\"",
    "sync-blog": "node sync-blog.js",
    "watch-sync": "node watch-and-sync.js",
    "create-site": "node create-site.js",
    "generate-site-registry": "node generate-site-registry.js",
    "download-images": "node download-blog-images.js all",
    "download-images:fastvistos": "node download-blog-images.js fastvistos",
    "download-images:p2digital": "node download-blog-images.js p2digital",
    "generate-blog": "node generate-blog-content.js",
    "generate-blog:watch": "node generate-blog-content.js --watch",
    "generate-blog:advanced": "node generate-blog-advanced.js",
    "test": "node test-blog-service.js --real",
    "test:blog": "node test-blog-service.js --real",
    "test:blog:real": "tsx test-blog-service.js --real",
    "test:blog:unit": "node test-blog-service.js --unit",
    "test:blog:verbose": "node test-blog-service.js --unit --verbose",
    "test:blog:integration": "node test-blog-service.js --integration",
    "dev:fastvistos": "node sync-blog.js fastvistos && SITE_ID=fastvistos astro dev",
    "dev:watch:fastvistos": "node dev-with-sync.js fastvistos",
    "build:fastvistos": "node sync-blog.js fastvistos && SITE_ID=fastvistos astro build && node postbuild-updatable.js fastvistos",
    "preview:fastvistos": "SITE_ID=fastvistos astro preview",
    "dev:p2digital": "node sync-blog.js p2digital && SITE_ID=p2digital astro dev",
    "dev:watch:p2digital": "node dev-with-sync.js p2digital",
    "build:p2digital": "node sync-blog.js p2digital && SITE_ID=p2digital astro build && node postbuild-updatable.js p2digital",
    "preview:p2digital": "SITE_ID=p2digital astro preview",
    "dev:zenith": "node sync-blog.js zenith && SITE_ID=zenith astro dev",
    "dev:watch:zenith": "node dev-with-sync.js zenith",
    "build:zenith": "node sync-blog.js zenith && SITE_ID=zenith astro build && node postbuild-updatable.js zenith",
    "preview:zenith": "SITE_ID=zenith astro preview",
    "dev:mysite": "node sync-blog.js mysite && SITE_ID=mysite astro dev",
    "dev:watch:mysite": "node dev-with-sync.js mysite",
    "build:mysite": "node sync-blog.js mysite && SITE_ID=mysite astro build && node postbuild-updatable.js mysite",
    "preview:mysite": "SITE_ID=mysite astro preview"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.4",
    "@astrojs/sitemap": "^3.5.1",
    "@prisma/client": "^6.15.0",
    "@tailwindcss/vite": "^4.1.13",
    "astro": "^5.13.5",
    "axios": "^1.11.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "glob": "^11.0.3",
    "prisma": "^6.15.0",
    "rehype-external-links": "^3.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-smartypants": "^3.0.2",
    "tailwindcss": "^4.1.13",
    "unist-util-visit": "^5.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "chokidar": "^4.0.3",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "tsx": "^4.20.5"
  }
}
