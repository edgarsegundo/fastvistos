    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "dev:fastvistos": "node sync-blog.js fastvistos && SITE_ID=fastvistos astro dev --config multi-sites.config.mjs",
    "dev:conceptvistos": "node sync-blog.js conceptvistos && SITE_ID=conceptvistos astro dev --config multi-sites.config.mjs",
    "dev:vibecode": "node sync-blog.js vibecode && SITE_ID=vibecode astro dev --config multi-sites.config.mjs",
    "dev:watch:fastvistos": "node dev-with-sync.js fastvistos",
