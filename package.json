    "@astrojs/sitemap": "^3.5.1",
    "@prisma/client": "^6.15.0",
    "@tailwindcss/vite": "^4.1.13",
    "astro": "^5.13.5",
    "glob": "^11.0.3",
    "prisma": "^6.15.0",
    "rehype-external-links": "^3.0.0",
    "remark-gfm": "^4.0.1",
    "remark-smartypants": "^3.0.2",
    "tailwindcss": "^4.1.13"
